       else if (nosound == false) {nosound = true; Print(s:"Mutilator sounds off");}
			
			}
	}



{
if (minigame == 1) //ARCADE GAME
   {
    if ((GetCVAR("SPECTIME") > 1) && (GetCVAR("CurrAssisterNum")== ConsolePlayerNumber())) // i don't CANDIDATE here 
