	delay(5);
	SetActorProperty(13300+PlayerNumber(), APROP_Alpha, 0.0);
	delay(5);
	SetActorProperty(13300+PlayerNumber(), APROP_Alpha, 1.0);
	SetActorProperty(13300+PlayerNumber(), APROP_Invulnerable, false);
	//}
}





