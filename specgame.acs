			HudMessage (s:"A"; HUDMSG_FADEOUT, 0, CR_GREY, 512.0, 384.0, 0.05, 0.5);
			}
			else if (minigame == 0) {minigame = 1; ChangeCamera (endcam[6], 0, 0); //Teleport(endcam[6]);
