if (!(ClassifyActor(13300+ConsolePlayerNumber()) & ACTOR_ALIVE))
{
NamedRequestScriptPuke("MGamerRespawn");
}
}
}



   if (buttonzz & BT_SPEED)
    {
      


	   if (PressUseAgain <=0)

