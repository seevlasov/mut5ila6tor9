			SetHUDSize(1024, 768, 0);
			SetFont("ZHAWHITE");
			HudMessage (s:"A"; HUDMSG_FADEOUT, 0, CR_GREY, 512.0, 384.0, 0.05, 0.5);
			//LIGHTEN MAP FOR SPECTATORS in mdi03
            if ((MUTILATOR_MOD() == 2) && (Client_action()))
            {
            for(int iai = 98; iai < 6501; iai++)  Light_ChangeToValue(iai, 0);
            }
			//PRINT(s:"A1");
       }
    else 

       {
	   if(GetCVAR("SPECTIME") == GetCVAR("defaultSPECTIME"))
           {
