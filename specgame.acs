////////////////INTRO
{
if ((PlayerIsSpectator(ConsolePlayerNumber())== 2) && (watchedintro == false) && (GetCVAR("MDISTATE")<3))
{
delay(70);
SetHUDSize(1024, 768, 0);
SetFont("ZHABLACK");
