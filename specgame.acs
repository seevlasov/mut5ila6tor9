delay(140);
SetFont("ZHABLACK");
HudMessage (s:"A"; HUDMSG_FADEINOUT, 0, CR_GREY, 512.0, 384.0, 0.05, 1.0, 1.0);
delay(35);
watchedintro = true;
minigame = 1; 
ChangeCamera (endcam[6], 0, 0);
