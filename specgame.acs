Script "HOUND_Forward" (void) NET 
{
int set; 
if (MUTILATOR_MOD() == 2) set = 3.0;
int velx, vely, velz;
velx = FixedMul(cos(GetActorAngle(8888)), FixedMul(cos(GetActorPitch(8888)), GetCvar("HoundSpeed") - set));
vely = FixedMul(sin(GetActorAngle(8888)), FixedMul(cos(GetActorPitch(8888)), GetCvar("HoundSpeed") - set));
SetActorVelocity(8888, velx, vely, -1.0, FALSE, FALSE);
//SetActorState(8888, "BASIC");
}

